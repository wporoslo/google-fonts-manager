{
  "name": "GFonts Manager",
  "version": "0.1",
  "description": "Google Fonts Missing Manager",
  "manifest_version": 2,
  "permissions": ["activeTab", "webNavigation"],
  "background": {
    "scripts": ["background.js", "runtime.js"]
  },
  "page_action": {
    "default_popup": "index.html"
  }
}
